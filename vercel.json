{
    "version": 2,
    "builds": [
        {
            "src": "Frontend/index.html",
            "use": "@vercel/static"
        },
        {
            "src": "api/*.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/Frontend/$1"
        }
    ]
}